{
  "name": "three-particles-editor",
  "version": "2.1.0",
  "type": "module",
  "private": false,
  "homepage": "https://github.com/NewKrok/three-particles-editor",
  "dependencies": {
    "@material/theme": "^14.0.0",
    "@newkrok/three-particles": "^2.3.0",
    "prismjs": "^1.30.0",
    "sass": "^1.97.3",
    "sirv-cli": "^3.0.1",
    "svelte-preprocess": "^6.0.3",
    "three": "^0.182.0"
  },
  "scripts": {
    "build": "npm run smui-theme-light && npm run smui-theme-dark && rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "husky",
    "smui-theme-light": "node scripts/compile-theme.js public/build/static/smui.css src/theme",
    "smui-theme-dark": "node scripts/compile-theme.js public/build/static/smui-dark.css src/theme --dark",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{js,svelte}\"",
    "test": "jest",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@smui/button": "^7.0.0",
    "@smui/card": "^7.0.0",
    "@smui/dialog": "^7.0.0",
    "@smui/list": "^7.0.0",
    "@smui/paper": "^7.0.0",
    "@smui/snackbar": "^7.0.0",
    "@smui/tab": "^7.0.0",
    "@smui/tab-bar": "^7.0.0",
    "@smui/textfield": "^7.0.0",
    "@types/jest": "^30.0.0",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "rollup": "^4.57.0",
    "rollup-plugin-css-only": "^4.5.5",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.3",
    "semantic-release": "^25.0.2",
    "sharp": "^0.34.5",
    "smui-theme": "^7.0.0",
    "svelte": "^4.2.20",
    "svelte-eslint-parser": "^1.4.1",
    "svrollbar": "^0.12.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
