{
  "name": "three-particles-editor",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://github.com/NewKrok/three-particles-editor",
  "dependencies": {
    "@newkrok/three-particles": "0.6.2",
    "node-sass": "^7.0.1",
    "sirv-cli": "^2.0.0",
    "svelte-preprocess": "^4.10.3",
    "three": "0.137.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@smui/button": "^6.0.0-beta.13",
    "@smui/card": "^6.0.0-beta.13",
    "eslint": "7.31.0",
    "prettier": "2.4.0",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0-beta.13",
    "svelte": "3.46.4"
  }
}
