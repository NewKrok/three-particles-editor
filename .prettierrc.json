{
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "svelteSortOrder": "options-scripts-markup-styles",
        "svelteStrictMode": false,
        "svelteIndentScriptAndStyle": true,
        "svelteBracketNewLine": true
      }
    }
  ]
}
